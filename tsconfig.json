{
  "compilerOptions": {
    "jsx": "react",
    "strictNullChecks": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": true,
    "target": "ES6",
    "lib": [
      "DOM",
      "ES2017"
    ],
    "skipLibCheck": true,
    "types": [
      "node"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}